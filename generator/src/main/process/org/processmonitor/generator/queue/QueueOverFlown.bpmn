        <activiti:out source="processDiagram" target="processDiagram"></activiti:out>
      </extensionElements>
    </callActivity>
    <scriptTask id="scripttask1" name="Script Task" scriptFormat="groovy">
      <script><![CDATA[	    counts = new HashMap<String, Object>();
	    counts.put( taskDefinitionKey , itemCount + "/" + limit);

	    highLightedActivities = new ArrayList<String>();
	    highLightedActivities.add( taskDefinitionKey );
	    
	    reportFileName="target/image.png";
]]></script>
    </scriptTask>
    <sequenceFlow id="flow3" name="" sourceRef="callactivity1" targetRef="script2"></sequenceFlow>
    <sequenceFlow id="flow4" name="" sourceRef="startevent1" targetRef="scripttask1"></sequenceFlow>
    <sequenceFlow id="flow5" name="" sourceRef="scripttask1" targetRef="callactivity1"></sequenceFlow>
    <scriptTask id="script2" name="Write image to file" scriptFormat="groovy">
      <script><![CDATA[javax.imageio.ImageIO.write( javax.imageio.ImageIO.read(new ByteArrayInputStream( processDiagram)),
				, "png"
				, new File( reportFileName));]]></script>
    </scriptTask>
    <sequenceFlow id="flow6" name="" sourceRef="script2" targetRef="endevent1"></sequenceFlow>
  </process>
